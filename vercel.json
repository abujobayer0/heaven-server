{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/gallery", "dest": "server.js" },
      { "src": "/animal-toys", "dest": "server.js" },
      { "src": "/art-and-craft-toys", "dest": "server.js" },
      { "src": "/police-car-toys", "dest": "server.js" },
      { "src": "/retailers", "dest": "server.js" },
      { "src": "/online-partners", "dest": "server.js" },
      { "src": "/featured-products", "dest": "server.js" },
      { "src": "/tranding-products", "dest": "server.js" },
      { "src": "/alltoys", "dest": "server.js" },
      { "src": "/toy/:id", "dest": "server.js" },
      { "src": "/user", "dest": "server.js" },
      { "src": "/add", "dest": "server.js" },
      { "src": "/update/:id", "dest": "server.js" },
      { "src": "/delete/:id", "dest": "server.js" },
      { "src": "/(.*)", "dest": "server.js" }
    ]
  }
  